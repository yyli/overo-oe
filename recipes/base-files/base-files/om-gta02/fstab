rootfs               /                    auto       defaults              1  1
proc                 /proc                proc       defaults              0  0
tmpfs                /var/volatile        tmpfs      defaults              0  0
tmpfs                /dev/shm             tmpfs      mode=0777             0  0
tmpfs                /media/ram           tmpfs      defaults              0  0

# microSD slot
/dev/mmcblk0p1       /media/card          auto       defaults              0  0
